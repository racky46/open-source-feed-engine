<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="FeedQueueType">

  <typeAlias alias="FeedQueueType" type="com.qagen.osfe.dataAccess.vo.FeedQueueType"/>

  <resultMap id="resultMap" class="FeedQueueType">
    <result property="feedQueueTypeId" column="feed_queue_type_id"/>
    <result property="maxConcurrentRun" column="max_concurrent_runs"/>
    <result property="description" column="description"/>
  </resultMap>

  <insert id="insert" parameterClass="FeedQueueType">
    insert into t_feed_queue_type (
      feed_queue_type_id,
      max_concurrent_runs,
      description
    ) values (
      #feedQueueTypeId#,
      #maxConcurrentRun#,
      #description#
    )
  </insert>

  <update id="update" parameterClass="FeedQueueType">
    update t_feed_queue_type set
      max_concurrent_runs = #maxConcurrentRun#,
      description = #description#
    where feed_queue_type_id = #feedQueueTypeId#
  </update>

  <delete id="delete" parameterClass="FeedQueueType">
    delete from t_feed_queue_type where feed_queue_type_id = #feedQueueTypeId#
  </delete>

  <select id="findByPrimaryId" parameterClass="string" resultMap="FeedQueueType.resultMap">
    select *from t_feed_queue_type where feed_queue_type_id = #feedQueueTypeId#
  </select>

  <select id="findAll" resultMap="FeedQueueType.resultMap">
    select *from t_feed_queue_type
  </select>

</sqlMap>