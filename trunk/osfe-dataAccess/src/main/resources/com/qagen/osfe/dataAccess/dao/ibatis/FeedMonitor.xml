<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="FeedMonitor">

  <typeAlias alias="FeedMonitor" type="com.qagen.osfe.dataAccess.vo.FeedMonitor"/>

  <resultMap id="resultMap" class="FeedMonitor">
    <result property="feedMonitorId" column="feed_monitor_id"/>
    <result property="description" column="description"/>
  </resultMap>

  <insert id="insert" parameterClass="FeedMonitor">
    insert into t_feed_monitor (
      feed_monitor_id,
      description
    ) values (
      #feedMonitorId#,
      #description#
    )
  </insert>

  <update id="update" parameterClass="FeedMonitor">
    update t_feed_monitor set
      feed_monitor_id = #feedMonitorId#,
      description = #description#
    where feed_monitor_id = #feedMonitorId#
  </update>

  <delete id="delete" parameterClass="FeedMonitor">
    delete from t_feed_monitor where feed_monitor_id = #feedMonitorId#
  </delete>

  <select id="findByPrimaryId" parameterClass="string" resultMap="FeedMonitor.resultMap">
    select * from t_feed_monitor where feed_monitor_id = #feedMonitorId#
  </select>

  <select id="findAll" resultMap="FeedMonitor.resultMap">
    select * from t_feed_monitor
  </select>

</sqlMap>