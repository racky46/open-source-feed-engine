<?xml version="1.0" encoding="UTF-8"?>
<feedDocument>

  <properties>
    <property name="someProperty" value="test"/>
  </properties>

  <delimitedRows minusRowCount="2" delimiterValue=",">
    <delimitedRow name="header" rowCount="1">
      <delimitedColumn name="examId" description="Uniquely Identifies The Exam" type="Long"/>
      <delimitedColumn name="courseId" description="Course Description" type="Integer"/>
      <delimitedColumn name="timestamp" description="File Create TimeStamp" type="Timestamp" format="yyyyMMdd-HH:mm:ss"/>
    </delimitedRow>

    <delimitedRow name="detail" linesToSkip="0">
      <delimitedColumn name="studentId" type="Long"/>
      <delimitedColumn name="lastName" type="String"/>
      <delimitedColumn name="firstName" type="String"/>
      <delimitedColumn name="middleInit" type="String"/>
      <delimitedColumn name="score" type="Integer"/>
    </delimitedRow>

    <delimitedRow name="footer" rowCount="1">
      <delimitedColumn name="examId" description="Uniquely Identifies The Exam" type="Long"/>
      <delimitedColumn name="courseId" description="Course Description" type="Integer"/>
      <delimitedColumn name="timestamp" description="File Create TimeStamp" type="Timestamp" format="yyyyMMdd-HH:mm:ss"/>
      <delimitedColumn name="rowCount" description="Row_Count" type="Integer"/>
    </delimitedRow>
  </delimitedRows>

  <phases batchSize="10000">
    <preFeedFilePhases>
       <phase name="fileNameValidationPhase" enable="true" className="com.acme.phases.FileNameValidationPhase"/>
    </preFeedFilePhases>

    <preEventPhases>
      <phase name="footerPhase" enable="true" className="com.acme.phases.FooterPhase"/>
      <phase name="headerPhase" enable="true" className="com.acme.phases.HeaderPhase"/>
    </preEventPhases>

    <batchEventPhases>
      <phase name="beanPopulatorPhase" enable="true" className="com.acme.phases.BeanPopulatorPhase"/>
      <phase name="gradingPhase" enable="true" className="com.acme.phases.GradingPhase">
        <properties>
          <property name="echoDetail" value="true"/>
        </properties>
      </phase>
      <phase name="checkpoint" enable="true" className="com.qagen.feedEngine.core.phases.CheckPointPhase"/>
    </batchEventPhases>

    <postEventPhases>
      <phase name="statsPhase" enable="true" className="com.acme.phases.StatsPhase"/>
    </postEventPhases>
  </phases>

  <splitters>
    <header>com.qagen.feedEngine.core.delimited.DelimitedHeaderSplitter</header>
    <detail>com.qagen.feedEngine.core.delimited.DelimitedDetailSplitter</detail>
    <footer>com.qagen.feedEngine.core.delimited.DelimitedFooterSplitter</footer>
  </splitters>

  <services>
    <mainService>com.qagen.feedEngine.core.services.StandardFeedLifeCycleService</mainService>
    <phaseService>com.qagen.feedEngine.core.services.PhaseLoaderService</phaseService>
    <exceptionService>com.qagen.feedEngine.core.services.ExceptionHandlerPlaceHolder</exceptionService>
  </services>

  <configLoaders>
    <propertyLoader>com.qagen.feedEngine.core.loaders.PropertiesConfigLoader</propertyLoader>
    <phaseLoader>com.qagen.feedEngine.core.loaders.PhaseConfigLoader</phaseLoader>
    <serviceLoader>com.qagen.feedEngine.core.loaders.ServiceConfigLoader</serviceLoader>
    <splitterLoader>com.qagen.feedEngine.core.loaders.SplitterConfigLoader</splitterLoader>
    <rowDescriptionLoader>com.qagen.feedEngine.core.delimited.DelimitedRowDescriptionLoader</rowDescriptionLoader>
  </configLoaders>

</feedDocument>
